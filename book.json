{
  "title": "note",
  "language": "zh-hans",
  "plugins": [
    "icp",
    "search-pro@2.0.2",
    "splitter@0.0.8",
    "toggle-chapters@0.0.3",
    "sectionx@3.1.0",
    "codeblock-filename@0.0.9",
    "donate@1.0.2",
    "back-to-top-button@0.1.4",
    "-expandable-chapters",
    "chapter-fold@0.0.4",
    "code@0.1.0",
    "page-copyright@1.0.8",
    "simple-page-toc@0.1.2",
    "page-toc-button@0.1.1",
    "flexible-alerts@1.0.3",
    "auto-scroll-table@0.0.5",
    "pageview-count@1.0.1",
    "popup@0.0.1",
    "insert-logo@0.1.5",
    "ad@0.0.4",
    "include-html@0.2.0",
    "-include@0.1.0",
    "hide-element@0.0.4",
    "terminal@0.3.2"
  ],
  "pluginsConfig": {
    "icp": {
      "number": "京ICP备20026373号",
      "style":{
        "display": "block",
        "color":"green"
      }
    },
    "search-pro": {
      "cutWordLib": "nodejieba",
      "defineWord": [
        "Gitbook Use"
      ]
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy 吕磊 2020",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "donate": {
      "wechat": "/image/common/pay.jpg",
      "title": "感谢赞助",
      "button": "赏",
      "alipayText": "微信打赏",
      "wechatText": "微信打赏"
    },
    "page-copyright": {
      "description": "文档修改于",
      "signature": "吕磊",
      "wisdom": "开发者 & 文档维护者 吕磊",
      "format": "YYYY-MM-dd hh:mm:ss",
      "copyright": "版权所有 &#169; 吕磊",
      "timeColor": "#666",
      "copyrightColor": "#666",
      "utcOffset": "8",
      "style": "normal",
      "noPowered": true,
      "baseUri": "https://github.com/click-one/note",
      "repo": "note"
    },
    "page-toc-button": {
      "maxTocDepth": 5,
      "minTocSize": 5
    },
    "flexible-alerts": {
      "note": {
        "label": {
          "de": "Hinweis",
          "en": "Note"
        }
      },
      "tip": {
        "label": {
          "de": "Tipp",
          "en": "Tip"
        }
      },
      "warning": {
        "label": {
          "de": "Warnung",
          "en": "Warning"
        }
      },
      "danger": {
        "label": {
          "de": "Achtung",
          "en": "Attention"
        }
      }
    },
    "hide-element": {
      "elements": [
        ".gitbook-link11"
      ]
    },
    "insert-logo": {
      "url": "/image/common/head.jpeg",
      "style": "background: none; max-height: 80px; min-height: 80px"
    },
    "ad": {
      "contentTop": "",
      "contentBottom": ""
    },
    "terminal": {
      "copyButtons": true,
      "fade": false,
      "style": "flat"
    }
  }
}
